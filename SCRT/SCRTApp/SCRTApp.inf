[defines]
BASE_NAME            = SCRTAPP
FILE_GUID            = 3886A4E6-5092-424e-BD71-2389F5C7C1DE
COMPONENT_TYPE       = APPLICATION

[sources.ARM]
  Arm/VirtualMemory.c
  Arm/GoVirtual.asm | RVCT
  Arm/GoVirtual.S | GCC

[sources.Aarch64]
  Aarch64/VirtualMemory.c
  Aarch64/GoVirtual.asm | RVCT
  Aarch64/GoVirtual.S | GCC

[sources.ia32]
  ia32/VirtualMemory.c
  ia32/GoVirtual.asm
 
[sources.x64]
  x64/VirtualMemory.c
  x64/GoVirtual.asm

[sources.ipf]
  ipf/VirtualMemory.c
  ipf/GoVirtual.s
  ipf/CpuIva.c
  ipf/IpfIvaHand.s
  ipf/IpfCpuCache.s
  
[sources.common]
  SCRTApp.c
  SCRTApp.h
  ParseConf.c
  ParseConf.h
  Debug.c
  Debug.h
  ParseResult.c

[sources.ARM, sources.Aarch64]
  Print.c

[includes.common]
  $(EDK_SOURCE)/Foundation
  $(EDK_SOURCE)/Foundation/Efi
  $(EDK_SOURCE)/Foundation/Framework
  $(EDK_SOURCE)/Other/Maintained/Application/Shell/Inc
  $(EDK_SOURCE)/Other/Maintained/Application/Shell/Library
  .
  $(EDK_SOURCE)/Foundation/Include
  $(EDK_SOURCE)/Foundation/Efi/Include
  $(EDK_SOURCE)/Foundation/Framework/Include
  $(EDK_SOURCE)/Foundation/Include/IndustryStandard
  $(EDK_SOURCE)/Foundation/Core/Dxe
  $(EDK_SOURCE)/Foundation/Library/Dxe/Include
  $(EFI_SOURCE)/SctPkg/Include

[libraries.common]
  SctLib
  
[libraries.IA32]
  PrintLib

[libraries.x64]
  PrintLib

[libraries.IPF]
  PrintLib

[libraries.ARM, libraries.Aarch64]
  CompilerIntrinsicsLib

[nmake.common]
  IMAGE_ENTRY_POINT=InitializeSCRT

